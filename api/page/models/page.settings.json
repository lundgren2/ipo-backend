{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "slug": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "excerpt": {
      "type": "text"
    },
    "publish_date": {
      "type": "datetime"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "permission"
    },
    "original_date": {
      "type": "datetime"
    },
    "wp_id": {
      "type": "integer"
    },
    "tag": {
      "model": "tag",
      "via": "pages"
    },
    "feature_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images", "files", "videos"],
      "plugin": "upload",
      "required": false
    }
  }
}
