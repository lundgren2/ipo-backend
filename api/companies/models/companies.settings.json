{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "name": "Companies",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "country": {
      "type": "enumeration",
      "enum": ["SE", "DK", "NO", "FI", "DE", "NL", "ISL"]
    },
    "name": {
      "type": "string"
    },
    "ipo": {
      "via": "company",
      "model": "ipo"
    },
    "podcast_episodes": {
      "via": "companies",
      "collection": "podcast-episode",
      "dominant": true
    },
    "market": {
      "type": "enumeration",
      "enum": [
        "first_north",
        "first_north_premier",
        "spotlight",
        "ngm_nordic_mtf",
        "ngm_sme",
        "nasdaq_omx_stockholm",
        "nasdaq_mid_cap",
        "nasdaq_small_cap",
        "alternativa_listan",
        "pepins"
      ]
    },
    "media": {
      "collection": "file",
      "via": "related",
      "allowedTypes": ["images", "files", "videos"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "about": {
      "type": "text"
    },
    "industry": {
      "type": "string"
    },
    "interview": {
      "type": "string"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "general.link"
    },
    "sector": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "website": {
      "type": "string",
      "default": "https://",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "articles": {
      "via": "companies",
      "collection": "article",
      "dominant": true
    }
  }
}
