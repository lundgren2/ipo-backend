{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "richtext"
    },
    "categories": {
      "collection": "category",
      "via": "articles",
      "dominant": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "excerpt": {
      "type": "text"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "date": {
      "type": "datetime",
      "required": false
    },
    "wp_id": {
      "type": "integer"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "tags": {
      "collection": "tag",
      "via": "articles",
      "dominant": true
    },
    "source": {
      "type": "string",
      "default": "ipo.se"
    },
    "article_url": {
      "type": "string"
    },
    "source_id": {
      "type": "string",
      "private": false,
      "unique": true
    },
    "companies": {
      "via": "articles",
      "collection": "company",
      "dominant": true
    },
    "sponsored": {
      "type": "boolean"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "media": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "general.link"
    },
    "files": {
      "type": "component",
      "repeatable": true,
      "component": "general.link"
    }
  }
}
