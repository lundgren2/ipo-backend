{
  "kind": "collectionType",
  "collectionName": "ipos",
  "info": {
    "name": "IPO",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "publish_date": {
      "type": "datetime",
      "required": false
    },
    "slug": {
      "type": "string"
    },
    "volume": {
      "type": "decimal"
    },
    "first_trading_day": {
      "type": "date"
    },
    "subscription_period": {
      "type": "component",
      "repeatable": false,
      "component": "ipo.subscription-period"
    },
    "price_per_share": {
      "type": "decimal"
    },
    "pre_money": {
      "type": "integer"
    },
    "min_subscription": {
      "type": "integer"
    },
    "prospectus": {
      "type": "string"
    },
    "guaranteed": {
      "type": "string"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "general.link"
    },
    "feature_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images", "videos"],
      "plugin": "upload",
      "required": false
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": ["files", "images", "videos"],
      "plugin": "upload",
      "required": false
    },
    "subscribe_buttons": {
      "type": "boolean",
      "default": true
    },
    "currency": {
      "type": "enumeration",
      "enum": ["SEK", "DKK", "EUR", "GBP", "NOK", "USD", "ISK"]
    },
    "subscription_commitment": {
      "type": "string"
    },
    "post_money": {
      "type": "string"
    },
    "wp_id": {
      "type": "integer"
    },
    "company": {
      "via": "ipo",
      "model": "companies"
    }
  }
}
