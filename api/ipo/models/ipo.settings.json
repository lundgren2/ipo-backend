{
  "kind": "collectionType",
  "collectionName": "ipos",
  "info": {
    "name": "IPO",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "volume": {
      "type": "decimal"
    },
    "first_trading_day": {
      "type": "date"
    },
    "price_per_share": {
      "type": "decimal"
    },
    "min_subscription": {
      "type": "decimal"
    },
    "prospectus": {
      "type": "string"
    },
    "guaranteed": {
      "type": "string"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "general.link"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images", "videos"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "subscribe_buttons": {
      "type": "boolean",
      "default": true
    },
    "currency": {
      "type": "enumeration",
      "enum": ["SEK", "DKK", "EUR", "GBP", "NOK", "USD", "ISK"]
    },
    "subscription_commitment": {
      "type": "string"
    },
    "wp_id": {
      "type": "integer"
    },
    "company": {
      "via": "ipo",
      "model": "company"
    },
    "featured": {
      "type": "boolean"
    },
    "sponsored": {
      "type": "boolean"
    },
    "subscription_period_from": {
      "type": "datetime"
    },
    "subscription_period_to": {
      "type": "datetime"
    },
    "pre_money": {
      "type": "decimal"
    },
    "post_money": {
      "type": "decimal"
    },
    "cancelled": {
      "type": "boolean",
      "default": false
    },
    "nordnet_link": {
      "type": "string",
      "default": "https://www.nordnet.se/foretagshandelser"
    },
    "avanza_link": {
      "type": "string",
      "default": "https://www.avanza.se/borsintroduktioner-emissioner.html"
    }
  }
}
