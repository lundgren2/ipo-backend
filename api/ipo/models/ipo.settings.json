{
  "kind": "collectionType",
  "collectionName": "ipos",
  "info": {
    "name": "IPO",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "publish_date": {
      "type": "datetime",
      "required": false
    },
    "slug": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "interview": {
      "type": "string"
    },
    "volume": {
      "type": "decimal"
    },
    "first_trading_day": {
      "type": "date"
    },
    "subscription_period": {
      "type": "component",
      "repeatable": false,
      "component": "ipo.subscription-period"
    },
    "price_per_share": {
      "type": "decimal"
    },
    "pre_money": {
      "type": "integer"
    },
    "min_subscription": {
      "type": "integer"
    },
    "prospectus": {
      "type": "string"
    },
    "website": {
      "type": "string",
      "default": "https://",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "guaranteed": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "country": {
      "type": "enumeration",
      "enum": ["SE", "DK", "NO", "FI", "DE", "NL", "ISL"]
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "general.link"
    },
    "feature_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images", "videos"],
      "plugin": "upload",
      "required": false
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": ["files", "images", "videos"],
      "plugin": "upload",
      "required": false
    },
    "logotype": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images"],
      "plugin": "upload",
      "required": false
    },
    "subscribe_buttons": {
      "type": "boolean",
      "default": true
    },
    "market": {
      "type": "enumeration",
      "enum": [
        "first_north",
        "first_north_premier",
        "spotlight",
        "ngm_nordic_mtf",
        "ngm_sme",
        "nasdaq_omx_stockholm",
        "nasdaq_mid_cap",
        "nasdaq_small_cap",
        "alternativa_listan",
        "pepins"
      ]
    },
    "sector": {
      "type": "string"
    },
    "currency": {
      "type": "enumeration",
      "enum": ["SEK", "DKK", "EUR", "GBP", "NOK", "USD", "ISK"]
    },
    "subscription_commitment": {
      "type": "string"
    },
    "post_money": {
      "type": "string"
    },
    "wp_id": {
      "type": "integer"
    }
  }
}
