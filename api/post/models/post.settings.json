{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "categories": {
      "collection": "category",
      "via": "post"
    },
    "slug": {
      "type": "string"
    },
    "excerpt": {
      "type": "string"
    },
    "publish_date": {
      "type": "datetime"
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "original_date": {
      "type": "datetime"
    },
    "wp_id": {
      "type": "integer"
    },
    "feature_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "tag": {
      "model": "tag",
      "via": "posts"
    }
  }
}
