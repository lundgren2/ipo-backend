{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "categories": {
      "via": "post",
      "collection": "category"
    },
    "slug": {
      "type": "string"
    },
    "excerpt": {
      "type": "string"
    },
    "publish_date": {
      "type": "datetime"
    },
    "published": {
      "type": "boolean",
      "default": true
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "original_date": {
      "type": "datetime"
    },
    "wp_id": {
      "type": "integer"
    },
    "feature_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "tag": {
      "via": "posts",
      "model": "tag"
    }
  }
}
